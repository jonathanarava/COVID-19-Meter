{
    "manifest_version": 1,
    "name": "COVID-19 Meter",
    "description": "the option to see more or less of COVID",
    "version": "1.0",
    "permissions": ["tabs"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"],
        "css": ["style.css"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "/images/image.png",
        "default_title": "(NO) more COVID-19",
        "default_popup": "/pop/popup.html"
    }
}